<h1>Listing exchanges</h1>

<table>
  <tr>
    <th>No</th>
    <th>Address</th>
    <th>Province</th>
    <th>City</th>
    <th>Region</th>
    <th>Zip</th>
    <th>Phone</th>
    <th>Memo</th>
    <th>Expected time</th>
    <th>Count</th>
    <th>Status</th>
    <th>Fullname</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exchanges.each do |exchange| %>
  <tr>
    <td><%= exchange.no %></td>
    <td><%= exchange.address %></td>
    <td><%= exchange.province %></td>
    <td><%= exchange.city %></td>
    <td><%= exchange.region %></td>
    <td><%= exchange.zip %></td>
    <td><%= exchange.phone %></td>
    <td><%= exchange.memo %></td>
    <td><%= exchange.expected_time %></td>
    <td><%= exchange.count %></td>
    <td><%= exchange.status %></td>
    <td><%= exchange.fullname %></td>
    <td><%= link_to 'Show', exchange %></td>
    <td><%= link_to 'Edit', edit_exchange_path(exchange) %></td>
    <td><%= link_to 'Destroy', exchange, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Exchange', new_exchange_path %>
