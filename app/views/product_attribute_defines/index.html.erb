<h1>Listing product_attribute_defines</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Short</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_attribute_defines.each do |product_attribute_define| %>
  <tr>
    <td><%= product_attribute_define.name %></td>
    <td><%= product_attribute_define.short %></td>
    <td><%= product_attribute_define.description %></td>
    <td><%= link_to 'Show', product_attribute_define %></td>
    <td><%= link_to 'Edit', edit_product_attribute_define_path(product_attribute_define) %></td>
    <td><%= link_to 'Destroy', product_attribute_define, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

